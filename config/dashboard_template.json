{
  "title": "销售分析仪表盘",
  "description": "月度销售数据分析与可视化",
  "layout": {
    "columns": 12,
    "row_height": 100
  },
  "datasources": {
    "sales_data": {
      "type": "csv",
      "file_path": "data/sample_sales.csv"
    }
  },
  "charts": [
    {
      "id": "revenue_trend",
      "type": "line",
      "title": "月度收入趋势",
      "datasource": "sales_data",
      "config": {
        "x": "date",
        "y": "sales",
        "agg_func": "sum",
        "color_theme": "business",
        "show_data_labels": true
      },
      "position": {
        "x": 0,
        "y": 0,
        "w": 8,
        "h": 4
      }
    },
    {
      "id": "category_distribution",
      "type": "pie",
      "title": "产品类别占比",
      "datasource": "sales_data",
      "config": {
        "names": "category",
        "values": "sales",
        "color_theme": "ocean",
        "hole": 0.4
      },
      "position": {
        "x": 8,
        "y": 0,
        "w": 4,
        "h": 4
      }
    },
    {
      "id": "region_comparison",
      "type": "bar",
      "title": "地区销售对比",
      "datasource": "sales_data",
      "config": {
        "x": "region",
        "y": "sales",
        "agg_func": "sum",
        "color_theme": "default",
        "orientation": "v"
      },
      "position": {
        "x": 0,
        "y": 4,
        "w": 6,
        "h": 4
      }
    },
    {
      "id": "top_products",
      "type": "table",
      "title": "热销产品 TOP 10",
      "datasource": "sales_data",
      "config": {
        "columns": ["product", "sales", "quantity"],
        "limit": 10,
        "sort_by": "sales",
        "ascending": false
      },
      "position": {
        "x": 6,
        "y": 4,
        "w": 6,
        "h": 4
      }
    }
  ],
  "filters": [
    {
      "id": "date_filter",
      "type": "date_range",
      "label": "时间范围",
      "column": "date",
      "default": "last_30_days"
    },
    {
      "id": "region_filter",
      "type": "category",
      "label": "地区",
      "column": "region",
      "multi_select": true
    }
  ],
  "refresh_interval": null,
  "created_at": "2025-11-10",
  "updated_at": "2025-11-10"
}
